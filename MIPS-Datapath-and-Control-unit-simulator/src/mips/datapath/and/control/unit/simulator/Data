package mips.datapath.and.control.unit.simulator;

public class Data {
    private int address;
    private int value;
    
    public Data(int mAddress, int mValue) {
        address = mAddress;
        value = mValue;
    }
    
    public int getAddress() {
        return address;
    }
    
    public int getValue() {
        return value;
    }
}
